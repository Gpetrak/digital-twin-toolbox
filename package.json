  "devDependencies": {
    "@turf/turf": "^6.5.0",
    "buffer": "^6.0.3",
    "cesium": "^1.111.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "earcut": "^2.2.4",
    "express": "^4.18.2",
    "filtrex": "^3.0.0",
    "lil-gui": "^0.19.1",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5",
    "shpjs": "^4.0.4",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "style-loader": "^3.3.3",
    "three": "^0.158.0",
    "uuid": "^9.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
