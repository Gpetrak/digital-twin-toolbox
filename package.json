{
  "name": "digital-twin-toolbox",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run setup && npm run client:build && npm run server:start",
    "start:dev": "npm run setup && concurrently -n client,server \"npm:client:dev\" \"npm:server:dev\"",
    "server:start": "node ./src/server/index.js",
    "server:dev": "nodemon ./src/server/index.js --legacy-watch --ignore ./static --ignore ./tmp",
    "client:build": "webpack --mode=production",
    "client:dev": "webpack --watch --mode=development",
    "setup": "node ./setup.js"
  },
  "keywords": [],
  "author": "",
